import("//build/package.gni")

executable("bin") {
  output_name = "chrealm"

  sources = [
    "main.cc",
  ]

  deps = [
    "//garnet/lib/chrealm",
    "//garnet/public/lib/fxl",
  ]
}

package("chrealm") {
  deprecated_system_image = true

  deps = [
    ":bin",
  ]

  binaries = [
    {
      name = "chrealm"
    },
  ]
}

