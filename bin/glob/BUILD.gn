import("//build/package.gni")

executable("bin") {
  output_name = "glob"

  sources = [
    "glob.cc",
  ]
}

package("glob") {
  deps = [
    ":bin",
  ]

  binary = "glob"

  meta = [
    {
      path = rebase_path("meta/glob.cmx")
      dest = "glob.cmx"
    },
  ]
}
