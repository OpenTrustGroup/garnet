import("//build/package.gni")

package("runmany") {
  deprecated_system_image = true

  deps = [
    ":runn",
  ]

  binaries = [
    {
      name = "runmany"
    },
  ]
}

executable("runn") {
  output_name = "runmany"
  sources = [
    "src/runmany.c",
  ]
  deps = [
    "//zircon/public/lib/fdio",
    "//zircon/public/lib/launchpad",
    "//zircon/public/lib/zx",
  ]
}
