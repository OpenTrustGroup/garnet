group("test") {
  testonly = true

  public_deps = [
    ":fake_command",
    ":fake_failing_test",
    ":fake_passing_test",
  ]
}

executable("fake_command") {
  testonly = true

  sources = [
    "fake_command.cc",
  ]
}

executable("fake_failing_test") {
  testonly = true

  deps = [
    "//third_party/googletest:gtest_main",
  ]

  sources = [
    "fake_failing_test.cc",
  ]
}

executable("fake_passing_test") {
  testonly = true

  deps = [
    "//third_party/googletest:gtest_main",
  ]

  sources = [
    "fake_passing_test.cc",
  ]
}
