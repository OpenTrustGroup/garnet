import("//build/package.gni")

executable("bin") {
  output_name = "trace_stress"

  sources = [
    "main.cc",
  ]

  deps = [
    "//garnet/public/lib/fxl",
    "//zircon/public/lib/async-loop-cpp",
    "//zircon/public/lib/trace-provider",
  ]
}

package("trace_stress") {
  deps = [
    ":bin",
  ]

  binaries = [ {
    name = "trace_stress"
  } ]
}
