import("//build/fidl/fidl.gni")
import("//build/package.gni")

package("mediaplayer") {
  package_name = "mediaplayer_examples"
  deprecated_system_image = true

  deps = [
    "audio_player",
    "audio_player_rust",
  ]

  binaries = [
    {
      name = "audio_player"
    },
    {
      name = "rust_crates/audio_player_rust"
      dest = "audio_player_rust"
    },
  ]
}
