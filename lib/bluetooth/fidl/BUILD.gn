  name = "bluetooth_host"
  sources = [
    "host.fidl",
  ]

  deps = [
    "//garnet/public/lib/bluetooth/fidl:bluetooth",
    "//garnet/public/lib/bluetooth/fidl:bluetooth_control",
    "//garnet/public/lib/bluetooth/fidl:bluetooth_gatt",
