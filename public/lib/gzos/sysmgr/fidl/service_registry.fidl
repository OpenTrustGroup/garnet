library sysmgr;

[Discoverable]
interface ServiceRegistry {
  1: AddService(string service_name);

  2: RemoveService(string service_name);

  3: LookupService(string service_name) -> (bool found);

  4: WaitOnService(string service_name) -> ();

  5: CancelWaitOnService(string service_name);
};
