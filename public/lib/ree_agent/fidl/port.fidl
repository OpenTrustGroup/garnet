library ree_agent;

interface TipcPort {
  // Called by the port manager when there has an incoming connection request
  1: OnConnectionRequest(request<TipcChannel>? channel);
};

[Discoverable]
interface TipcPortManager {
  // Publish a Port to PortManager
  1: Publish(string port_path, TipcPort? port) -> (status s);
};
