library sketchy;

using gfx;

struct Touch {
  gfx.vec2 position;
  uint32 nanoseconds;
  float32 pressure;
};

struct CubicBezier2 {
  gfx.vec2 pt0;
  gfx.vec2 pt1;
  gfx.vec2 pt2;
  gfx.vec2 pt3;
};

struct StrokePath {
  vector<CubicBezier2> segments;
};
