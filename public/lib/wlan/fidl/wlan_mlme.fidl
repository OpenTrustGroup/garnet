    DEAUTHENTICATE_request = 9;

    // MLME-DEAUTHENTICATE.confirm
    // Args: DeauthenticateConfirm
    DEAUTHENTICATE_confirm = 10;

    // MLME-DEAUTHENTICATE.indication
    // Args: DeauthenticateIndication
